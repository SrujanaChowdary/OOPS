#include<iostream>
using namespace std;
int main() 
{
  float amount, rate, t, si;
 
  cout<<"Enter Principal Amount: ";
  cin>>amount;
 
  cout<<"Enter Rate of Interest: ";
  cin>>rate;
 
  cout<<"Enter Period of Time: ";
  cin>>t;
 
  si = (amount * rate * t) / 100;
  cout<<"Simple Interest: "<<si;
  
 return 0;
}
