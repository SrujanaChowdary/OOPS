#include <iostream>
using namespace std;
int sum(int a,int b)
{
    return (a+b);
}
int sum(int a,int b,int c)
{
    return (a+b+c);
}
int main()
{
    int a, b;
    cout<<"enter Two values:";
    cin>>a>>b;
    cout<<"sum of "<<a<<" "<<b<<" :"<<sum(a,b)<<endl;
    int c;
    cout<<"enter a value:";
    cin>>c;
    cout<<"sum of "<<a<<" "<<b<<" "<<c<<" :"<<sum(a,b,c)<<endl;
}
